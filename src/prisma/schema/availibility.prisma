model Availability {
    id        String   @id @default(uuid())
    tutorId   String
    tutor     Tutor    @relation(fields: [tutorId], references: [userId], onDelete: Cascade)
    dayOfWeek Int
    startTime String
    endTime   String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt

    @@index([tutorId])
    @@index([dayOfWeek])
}
